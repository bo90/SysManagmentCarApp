<Window x:Class="SysManagmentCarApp.Models.Report"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:rv="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        Title="Report" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <WindowsFormsHost Margin="10">
            <WindowsFormsHost.BindingGroup>
                <BindingGroup/>
            </WindowsFormsHost.BindingGroup>
            <rv:ReportViewer x:Name="_reportViwer" Load="_reportViwer_Load"/>
        </WindowsFormsHost>
    </Grid>
</Window>
